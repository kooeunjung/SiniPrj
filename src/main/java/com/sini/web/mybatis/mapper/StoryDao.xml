<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.sini.web.dao.StoryDao">    
	<select id="getList" resultType="com.sini.web.entity.Story" >
		SELECT * FROM sini.Story;
	</select>
	
	<select id="get" resultType="com.sini.web.entity.Story" >
		SELECT * FROM Story WHERE storyId=#{storyId}
	</select>
	
	
</mapper>